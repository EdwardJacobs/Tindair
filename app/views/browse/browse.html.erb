<ul id="slides">
  <% @users.each do |user| %>
  <li class="slide" data-id="<%= user.id %>">
    <div class="image-carousel" style="background-image: url('<%= cl_image_path user.photo.key, height: 300, width: 400, crop: :fill %>')">
      <p><%= user.first_name %></p>

    </div>

  </li>
  <% end %>
</ul>

<div id="slide-controls">
  <span class="controls" id="decline"><i class="fa fa-times-circle fa-2x"></i></span>
  <span class="controls" id="approve"><i class="fa fa-heart fa-2x"></i></span>
</div>
