<ul id="slides">
  <% @users.each do |user| %>
  <div class="slide" data-id="<%= user.id %>">
    <p><%= user.first_name %></p>
  </div>
  <% end %>
</ul>

<span class="controls" id="decline"><i class="fa fa check fa-2x"></i></span>
<span class="controls" id="approve"><i class="fa fa heart fa-2x"></i></span>
